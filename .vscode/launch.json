{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch and Attach to npm run dev",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "dev"],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Node Process",
			"port": 9229,
			"restart": true
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Run Current File",
			"program": "${file}",
			"console": "integratedTerminal"
		}
	]
}
